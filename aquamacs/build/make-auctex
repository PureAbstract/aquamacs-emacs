#!/bin/sh

echo "Call this with the auctex source distribution as current directory."

# cd ../../auctex*

./configure --prefix=/Users/dr/Projects/Aquamacs/aquamacs/src/site-lisp/edit-modes/ --with-emacs=/Applications/Aquamacs\ Emacs.app/Contents/MacOS/Emacs --with-lispdir=/Users/dr/Projects/Aquamacs/aquamacs/src/site-lisp/edit-modes --without-texmf-dir

